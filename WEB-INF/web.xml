        <servlet-name>pregunta</servlet-name>
        <url-pattern>/pregunta.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Preguntas</servlet-name>
        <url-pattern>/Preguntas.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>preguntas1</servlet-name>
        <url-pattern>/preguntas1.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>preguntas2</servlet-name>
