    <servlet-mapping>
        <servlet-name>preguntas3</servlet-name>
        <url-pattern>/preguntas3.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
